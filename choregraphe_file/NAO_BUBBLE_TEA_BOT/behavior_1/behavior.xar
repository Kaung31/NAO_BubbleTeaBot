              <script language="4">
                <content>
                  <![CDATA[import socket

host = '127.0.0.1'  #server's private IP address
port = 8888
client_socket = None

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.current_question = 1  # Tracks the current Q&A step

    def onLoad(self):
        """Establishes socket connection to the server."""
        global client_socket
        try:
            client_socket = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
            client_socket.connect((host, port))
            self.logger.info("Connected to the server.")
        except Exception as e:
            self.logger.error("Failed to connect to server: {}".format(e))
            client_socket = None

    def onInput_user_input(self, input_data):
        """
        Handles input received from the Text Edit Box.
        :param input_data: The text input from the user.
        """
        global client_socket
        try:
            # Handle empty input
            if not input_data:
                input_data = "default question"
                self.logger.warning("Input data is empty. Using default question.")

            self.logger.info("Processing input: {}".format(input_data))

            # Ensure the socket connection is active
            if client_socket is None:
                self.logger.error("No active socket connection. Cannot send data.")
                return

            # Send input to the server
            client_socket.sendall(input_data.encode())
            self.logger.info("Sent to server: {}".format(input_data))

            # Receive response
            response = client_socket.recv(1024).decode()
            self.logger.info("Received from server: {}".format(response))

            # Handle response: Check for speech and gesture split
            if "|" in response:
                speech, gesture = response.split("|", 1)
                self.logger.info("Speech: {}, Gesture: {}".format(speech, gesture))
                self.send_output("speech_output", str(speech))  # Send speech to Say Text output
                self.send_output(gesture + "_output")  # Trigger gesture output
            else:
                # If no gesture is present, send the speech only
                self.send_output("speech_output", str(response))

        except Exception as e:
            self.logger.error("Communication error: {}".format(e))

    def send_output(self, output_name, value=None):
        """
        Sends output to the corresponding connected box.
        :param output_name: The name of the output to trigger.
        :param value: The value to send (for speech or others).
        """
        try:
            if value is not None:
                self.logger.info("Sending output to {}: {}".format(output_name, value))
                if hasattr(self, output_name):
                    getattr(self, output_name)(value)
                else:
                    self.logger.warning("Output '{}' is not connected or defined.".format(output_name))
            else:
                self.logger.info("Sending output to {} without value.".format(output_name))
                if hasattr(self, output_name):
                    getattr(self, output_name)()
        except AttributeError as e:
            self.logger.error("Output not defined: {}. Error: {}".format(output_name, e))
        except Exception as e:
            self.logger.error("Error triggering output {}: {}".format(output_name, e))

    def onInput_onStop(self):
        """
        Closes the socket connection when the behavior stops.
        """
        global client_socket
        try:
            if client_socket:
                client_socket.close()
                self.logger.info("Socket connection closed.")
        except Exception as e:
            self.logger.error("Error closing socket: {}".format(e))
        self.onStopped()  # Activate the output of the box]]>
                </content>
